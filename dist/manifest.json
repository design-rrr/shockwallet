{
  "short_name": "SHOCKWALLET",
  "name": "SHOCKWALLET",
  "icons": [
    {
      "src": "favicon.png",
      "sizes": "64x64 32x32 24x24 16x16",
      "type": "image/x-icon"
    },
    {
      "src": "logo192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "logo512.png",
      "type": "image/png",
      "sizes": "512x512"
    }
  ],
  "start_url": "/",
  "display": "standalone",
  "theme_color": "#16191c",
  "background_color": "#16191c",
  "protocol_handlers": [
    {
      "protocol": "web+lightning",
      "url": "./?address=%s"
    },
    {
      "protocol": "bitcoin",
      "url": "./?address=%s"
    },
    {
      "protocol": "https",
      "host": "test.shockwallet.app",
      "url": "./?address=%s"
    }
  ],
  "intent_filters": [
    {
      "action": "VIEW",
      "autoVerify": true,
      "data": {
        "scheme": "bitcoin"
      }
    },
    {
      "action": "VIEW",
      "autoVerify": true,
      "data": {
        "scheme": "https",
        "host": "test.shockwallet.app"
      }
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://test.shockwallet.app/manifest.json"
    }
  ],
  "url_handlers": [
    {
      "origin": "https://test.shockwallet.app"
    },
    {
      "origin": "https://*.shockwallet.app"
    },
    {
      "orogin": "localhost"
    }
  ]
}